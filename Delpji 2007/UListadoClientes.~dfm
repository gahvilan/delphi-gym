  object IBQListadoClientes: TIBQuery
    Database = DMGimnasio.IBDGimnasio
    Transaction = DMGimnasio.IBTGimnasio
    BeforeOpen = IBQListadoClientesBeforeOpen
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select * from clientes where (establecimiento=:esta) and (nombre' +
        ' like :nombre) and (apellido like :apellido) and (edad between :' +
        'edad1 and :edad2)'
      ' ')
    Left = 80
    Top = 288
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'esta'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'nombre'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'apellido'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'edad1'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'edad2'
        ParamType = ptUnknown
      end>
  end
