  object IBQBuscarMes: TIBQuery
    Database = DMGimnasio.IBDGimnasio
    Transaction = DMGimnasio.IBTGimnasio
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select (select p.nombre||'#39' '#39'||p.apellido from personal p where (' +
        'a.id_personal=p.id_personal)) as Nombre, (select nombre from dis' +
        'iplinas d where (d.id_disiplina=a.id_disiplina)) as Disiplina, a' +
        '.Dia as Dia, a.Hora as Hora from acargo a where mes=:mes and ani' +
        'o=:anio')
    Left = 632
    Top = 225
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'mes'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'anio'
        ParamType = ptUnknown
      end>
  end
  object IBQEliminarClase: TIBQuery
    Database = DMGimnasio.IBDGimnasio
